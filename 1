#include <stdio.h>
#include <math.h>
#include <stdlib.h>

int main(void){
    
    float x,y,z;
    float s=0;
    printf("vvedite x,y,z: ");

    x=0.1722;
    y=6.33;
    z=3.25*pow(10,-4);
    printf ("%f \n",s);
    s=(5*atan(x))-(1/4*acos(x))*(x+3*abs(x-y)+(x*x))/(abs(x-y)*z+(x*x));
    printf("vvedennie znashediya: %f, %f, %f",x,y,z);
    printf(" \n rezult= %0.3f ",s);

    return 0;
}
